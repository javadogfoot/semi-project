package tomato.sell_re;

import java.sql.*;

public class SellReDTO {

	private int s_r_idx;
	private int s_idx;
	private int m_idx;
	private String s_r_con;
	private Date s_r_joindate;
	private int s_r_del;
	private int s_r_ref;
	private int s_r_lev;
	private int s_r_sunbun;
	
	public SellReDTO() {
		
	}

	public SellReDTO(int s_r_idx, int s_idx, int m_idx, String s_r_con, Date s_r_joindate, int s_r_del, int s_r_ref,
			int s_r_lev, int s_r_sunbun) {
		super();
		this.s_r_idx = s_r_idx;
		this.s_idx = s_idx;
		this.m_idx = m_idx;
		this.s_r_con = s_r_con;
		this.s_r_joindate = s_r_joindate;
		this.s_r_del = s_r_del;
		this.s_r_ref = s_r_ref;
		this.s_r_lev = s_r_lev;
		this.s_r_sunbun = s_r_sunbun;
	}

	public int getS_r_idx() {
		return s_r_idx;
	}

	public void setS_r_idx(int s_r_idx) {
		this.s_r_idx = s_r_idx;
	}

	public int getS_idx() {
		return s_idx;
	}

	public void setS_idx(int s_idx) {
		this.s_idx = s_idx;
	}

	public int getM_idx() {
		return m_idx;
	}

	public void setM_idx(int m_idx) {
		this.m_idx = m_idx;
	}

	public String getS_r_con() {
		return s_r_con;
	}

	public void setS_r_con(String s_r_con) {
		this.s_r_con = s_r_con;
	}

	public Date getS_r_joindate() {
		return s_r_joindate;
	}

	public void setS_r_joindate(Date s_r_joindate) {
		this.s_r_joindate = s_r_joindate;
	}

	public int getS_r_del() {
		return s_r_del;
	}

	public void setS_r_del(int s_r_del) {
		this.s_r_del = s_r_del;
	}

	public int getS_r_ref() {
		return s_r_ref;
	}

	public void setS_r_ref(int s_r_ref) {
		this.s_r_ref = s_r_ref;
	}

	public int getS_r_lev() {
		return s_r_lev;
	}

	public void setS_r_lev(int s_r_lev) {
		this.s_r_lev = s_r_lev;
	}

	public int getS_r_sunbun() {
		return s_r_sunbun;
	}

	public void setS_r_sunbun(int s_r_sunbun) {
		this.s_r_sunbun = s_r_sunbun;
	}
	
}