package tomato.lesson_re;

import java.sql.*;

public class LessonReDTO {
	private int l_r_idx;
	private int l_idx;
	private int m_idx;
	private String l_r_con;
	private Date l_r_joindate;
	private int l_r_del;
	private int l_r_ref;
	private int l_r_lev;
	private int l_r_sunbun;
	private String m_id; //추가정보
	private String m_name;	//추가정보
	

	public LessonReDTO() {
		super();
	}
	
	

	public LessonReDTO(int l_r_idx, int l_idx, int m_idx, String l_r_con, Date l_r_joindate, int l_r_del, int l_r_ref,
			int l_r_lev, int l_r_sunbun) {
		super();
		this.l_r_idx = l_r_idx;
		this.l_idx = l_idx;
		this.m_idx = m_idx;
		this.l_r_con = l_r_con;
		this.l_r_joindate = l_r_joindate;
		this.l_r_del = l_r_del;
		this.l_r_ref = l_r_ref;
		this.l_r_lev = l_r_lev;
		this.l_r_sunbun = l_r_sunbun;
	}


	public LessonReDTO(int l_r_idx, int l_idx, int m_idx, String l_r_con, Date l_r_joindate, int l_r_del, int l_r_ref,
			int l_r_lev, int l_r_sunbun, String m_id, String m_name) {
		super();
		this.l_r_idx = l_r_idx;
		this.l_idx = l_idx;
		this.m_idx = m_idx;
		this.l_r_con = l_r_con;
		this.l_r_joindate = l_r_joindate;
		this.l_r_del = l_r_del;
		this.l_r_ref = l_r_ref;
		this.l_r_lev = l_r_lev;
		this.l_r_sunbun = l_r_sunbun;
		this.m_id = m_id;
		this.m_name = m_name;
	}



	public int getL_r_idx() {
		return l_r_idx;
	}

	public void setL_r_idx(int l_r_idx) {
		this.l_r_idx = l_r_idx;
	}

	public int getL_idx() {
		return l_idx;
	}

	public void setL_idx(int l_idx) {
		this.l_idx = l_idx;
	}

	public int getM_idx() {
		return m_idx;
	}

	public void setM_idx(int m_idx) {
		this.m_idx = m_idx;
	}

	public String getL_r_con() {
		return l_r_con;
	}

	public void setL_r_con(String l_r_con) {
		this.l_r_con = l_r_con;
	}

	public Date getL_r_joindate() {
		return l_r_joindate;
	}

	public void setL_r_joindate(Date l_r_joindate) {
		this.l_r_joindate = l_r_joindate;
	}

	public int getL_r_del() {
		return l_r_del;
	}

	public void setL_r_del(int l_r_del) {
		this.l_r_del = l_r_del;
	}

	public int getL_r_ref() {
		return l_r_ref;
	}

	public void setL_r_ref(int l_r_ref) {
		this.l_r_ref = l_r_ref;
	}

	public int getL_r_lev() {
		return l_r_lev;
	}

	public void setL_r_lev(int l_r_lev) {
		this.l_r_lev = l_r_lev;
	}

	public int getL_r_sunbun() {
		return l_r_sunbun;
	}

	public void setL_r_sunbun(int l_r_sunbun) {
		this.l_r_sunbun = l_r_sunbun;
	}



	public String getM_id() {
		return m_id;
	}



	public void setM_id(String m_id) {
		this.m_id = m_id;
	}



	public String getM_name() {
		return m_name;
	}



	public void setM_name(String m_name) {
		this.m_name = m_name;
	}
	
	
}